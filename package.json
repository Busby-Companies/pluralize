{
  "name": "@capaj/pluralize",
  "version": "1.0.3",
  "description": "Pluralize and singularize any word",
  "source": "src/pluralize.ts",
  "types": "dist/pluralize.d.ts",
  "type": "module",
  "main": "./dist/pluralize.cjs",
  "module": "./dist/pluralize.module.js",
  "unpkg": "./dist/pluralize.umd.js",
  "files": [
    "dist",
