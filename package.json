  "readmeFilename": "readme.md",
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "husky": "^6.0.0",
    "istanbul": "^0.4.5",
    "microbundle": "^0.13.2",
    "mocha": "^5.0.0",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.0",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.3.2"
  },
  "husky": {
    "hooks": {
